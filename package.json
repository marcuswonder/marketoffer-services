{
  "name": "men-redis-scraper",
  "version": "0.2.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "nodemon --watch src --exec ts-node src/api/index.ts",
    "dev:worker:ch": "nodemon --watch src --exec ts-node src/workers/chAppointments.ts",
    "dev:worker:company": "nodemon --watch src --exec ts-node src/workers/companyDiscovery.ts",
    "dev:worker:person": "nodemon --watch src --exec ts-node src/workers/personLinkedin.ts",
    "dev:worker:owner": "nodemon --watch src --exec ts-node src/workers/ownerDiscovery.ts",
    "build": "tsc -p .",
    "start": "node dist/api/index.js",
    "preworker:ch": "npm run build",
    "worker:ch": "node dist/workers/chAppointments.js",
    "preworker:company": "npm run build",
    "worker:company": "node dist/workers/companyDiscovery.js",
    "preworker:sitefetch": "npm run build",
    "worker:sitefetch": "node dist/workers/siteFetch.js",
    "preworker:person": "npm run build",
    "worker:person": "node dist/workers/personLinkedin.js",
    "preworker:owner": "npm run build",
    "worker:owner": "node dist/workers/ownerDiscovery.js",
    "seed:hosts": "ts-node --esm scripts/seedGenericHosts.ts airtable_schema/generic_hosts.csv",
    "test:airtable": "ts-node --esm scripts/testAirtable.ts"
  },
  "dependencies": {
    "@bull-board/api": "^6.5.4",
    "@bull-board/express": "^6.5.4",
    "airtable": "^0.12.2",
    "bullmq": "^5.12.1",
    "cookie-parser": "^1.4.6",
    "csv-parser": "^3.2.0",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "ioredis": "^5.3.2",
    "p-limit": "^6.1.0",
    "pg": "^8.16.3",
    "pino": "^9.3.2",
    "undici": "^7.3.0",
    "unzipper": "^0.12.3",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.9",
    "@types/express": "^4.17.21",
    "@types/node": "^22.5.0",
    "@types/pg": "^8.15.5",
    "nodemon": "^3.1.4",
    "pino-pretty": "^11.2.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2"
  }
}
